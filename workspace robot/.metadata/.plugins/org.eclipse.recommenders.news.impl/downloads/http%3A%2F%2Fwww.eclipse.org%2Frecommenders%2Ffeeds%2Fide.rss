<script type="text/javascript">
var gTxtWaitmore = "Bitte warten Sie %1 Sekunden.";
var gTxtWaitone = "Bitte warten Sie 1 Sekunde.";
function block(blocktime)
{
var form = jxl.get("uiMainForm");
if (form)
{
if ( blocktime > 0 )
{
doBlockTicket(blocktime*1000);
}
}
}
function getWaitStr(mSec)
{
var sec = Math.ceil(mSec/1000);
if (sec === 1)
{
return gTxtWaitone;
}
else
{
return gTxtWaitmore.replace(/%1/,sec);
}
}
function doBlockTicket(time)
{
jxl.disable("uiTicket");
jxl.disable("uiSendTicket");
var start = (new Date()).getTime();
var timer = timer || null;
if (timer)
{
window.clearTimeout(timer);
timer = null;
}
function f()
{
var now = (new Date()).getTime();
var wait = time - (now - start);
if (wait > 0)
{
jxl.setHtml("uiWait", getWaitStr(wait));
jxl.show("uiLoginError");
timer = window.setTimeout(f, 500);
}
else
{
jxl.hide("uiLoginError");
jxl.enable("uiTicket");
jxl.enable("uiSendTicket");
}
}
f();
}
function init()
{
var blockTime = 0;
block(blockTime);
}
ready.onReady(init);
</script>
